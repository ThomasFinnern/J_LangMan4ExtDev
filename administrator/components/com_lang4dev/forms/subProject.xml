<?xml version="1.0" encoding="UTF-8"?>
<form>
	<field
			name="id"
			type="number"
			label="JGLOBAL_FIELD_ID_LABEL"
			default="0"
			class="readonly"
			readonly="true"
	/>

	<field
			name="title"
			type="text"
			label="COM_LANG4DEV_SUBPROJECT_TITLE"
			size="40"
	/>

	<field
			name="alias"
			type="text"
			label="JFIELD_ALIAS_LABEL"
			size="45"
			description="JFIELD_ALIAS_DESC"
			hint="JFIELD_ALIAS_PLACEHOLDER"
	/>

	<field name="prjId"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_ID"
	       description="COM_LANG4DEV_SUBPROJECT_ID_DESC" />

	<field name="type"
	       type="list"
	       label="COM_LANG4DEV_SUBPROJECT_TYPE"
	       description="COM_LANG4DEV_PROJECT_TYPE_DESC"
	>
		<option value="0">COM_LANG4DEV_PRJ_TYPE_NONE</option>
		<option value="1">COM_LANG4DEV_PRJ_TYPE_COMP_BACK_SYS</option>
		<option value="2">COM_LANG4DEV_PRJ_TYPE_COMP_BACK</option>
		<option value="3">COM_LANG4DEV_PRJ_TYPE_COMP_SITE</option>
		<option value="4">COM_LANG4DEV_PRJ_TYPE_MODEL</option>
		<option value="5">COM_LANG4DEV_PRJ_TYPE_PLUGIN</option>
	</field>

	<field name="root_path"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_ROOT_PATH"
	       description="COM_LANG4DEV_SUBPROJECT_ROOT_PATH_DESC"  />

	<field name="prjXmlFilePath"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_XML_FILE_PATH"
	       description="COM_LANG4DEV_SUBPROJECT_XML_FILE_PATH_DESC"  />

	<field
			name="twin_id"
			type="text"
			label="COM_LANG4DEV_SUBPROJECT_ORIGIN_TWIN_ID"
			description="COM_LANG4DEV_SUBPROJECT_ORIGIN_TWIN_ID_DESC"
			default="0"
			class="readonly"
	/>


	<field
			name="notes"
			type="textarea"
			label="COM_LANG4DEV_SUBPROJECT_NOTES"
			description="COM_LANG4DEV_SUBPROJECT_NOTES_DESC"
			size="40"
			maxlength="255"
	/>

	<field
			name="extension"
			type="hidden"
	/>

	<field
			name="ordering"
			type="integer"
			filter="unset"
	/>

	<field
			name="created"
			type="calendar"
			label="JGLOBAL_CREATED_DATE"
			translateformat="true"
			showtime="true"
			size="22"
			filter="user_utc"
	/>

	<field
			name="created_by"
			type="user"
			label="JGLOBAL_FIELD_CREATED_BY_LABEL"
	/>

	<field
			name="created_by_alias"
			type="text"
			label="COM_LANG4DEV_CREATED_BY_ALIAS_LABEL"
			size="20"
	/>

	<field
			name="modified"
			type="calendar"
			label="JGLOBAL_FIELD_MODIFIED_LABEL"
			class="form-control"
			translateformat="true"
			showtime="true"
			size="22"
			readonly="true"
			filter="user_utc"
	/>

	<field
			name="modified_by"
			type="user"
			label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
			class="readonly"
			readonly="true"
			filter="unset"
	/>

	<field
			name="checked_out"
			type="hidden"
			filter="unset"
	/>

	<field
			name="checked_out_time"
			type="hidden"
			filter="unset"
	/>

	<field
			name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			size="1"
	/>

	<!-- componentPrefix, prjXmlPathFilename, installPathFilename, isSysFiles -->


	<!-- ? $langFiles = [], $transIdLocations = [], $transIdsClassified; -->



</form>