<?xml version="1.0" encoding="UTF-8"?>
<form>
	<field
			name="id"
			type="hidden"
			label="JGLOBAL_FIELD_ID_LABEL"
			default="0"
			readonly="true"
	/>

	<field
			name="title"
			type="text"
			label="JGLOBAL_TITLE"
			size="40"
	/>

	<field
			name="alias"
			type="text"
			label="JFIELD_ALIAS_LABEL"
			size="45"
			description="JFIELD_ALIAS_DESC"
			hint="JFIELD_ALIAS_PLACEHOLDER"
	/>

	<field name="prjId"
	       type="text"
	       label="COM_LANG4DEV_COMPONENT_NAME"
	       description="COM_LANG4DEV_COMPONENT_NAME_DESC"
	       readonly="true"
	/>

	<field name="subPrjType"
	       type="list"
	       label="COM_LANG4DEV_SUBPROJECT_TYPE"
	       description="COM_LANG4DEV_SUBPROJECT_TYPE_DESC"
	>
		<option value="0">COM_LANG4DEV_PRJ_TYPE_NONE</option>
		<option value="1">COM_LANG4DEV_PRJ_TYPE_COMP_BACK_SYS</option>
		<option value="2">COM_LANG4DEV_PRJ_TYPE_COMP_BACK</option>
		<option value="3">COM_LANG4DEV_PRJ_TYPE_COMP_SITE</option>
		<option value="4">COM_LANG4DEV_PRJ_TYPE_MODULE</option>
		<option value="5">COM_LANG4DEV_PRJ_TYPE_PLUGIN</option>
		<option value="6">COM_LANG4DEV_PRJ_TYPE_WEB_ADMIN</option>
		<option value="7">COM_LANG4DEV_PRJ_TYPE_WEB_SITE</option>
		<option value="8">COM_LANG4DEV_PRJ_TYPE_TEMPLATE</option>
	</field>

	<field name="root_path"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_ROOT_PATH"
	       description="COM_LANG4DEV_SUBPROJECT_ROOT_PATH_DESC"
	/>

	<field name="prefix"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_LANG_PREFIX"
	       description="COM_LANG4DEV_SUBPROJECT_PREFIX_DESC"
	/>

	<field name="isLangAtStdJoomla"
	       type="radio"
	       default="1"
	       label="COM_LANG4DEV_SUBPROJECT_LANG_LOCATION_JOOMLA_STANDARD"
	       description="COM_LANG4DEV_SUBPROJECT_LANG_LOCATION_JOOMLA_STANDARD_DESC"
	       layout="joomla.form.field.radio.switcher"
	>
	       <option value="1">JYES</option>
	       <option value="0">JNO</option>
	</field>
		
	<field
			name="notes"
			type="textarea"
			label="COM_LANG4DEV_SUBPROJECT_NOTES"
			description="COM_LANG4DEV_SUBPROJECT_NOTES_DESC"
			
			size="40"
			maxlength="255"
	/>


	<field name="prjXmlPathFilename"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_XML_PATH_FILE_NAME"
	       description="COM_LANG4DEV_SUBPROJECT_XML_PATH_FILE_NAME_DESC"/>

	<field name="installPathFilename"
	       type="text"
	       label="COM_LANG4DEV_SUBPROJECT_INSTALL_PATH_FILE_NAME"
	       description="COM_LANG4DEV_SUBPROJECT_INSTALL_PATH_FILE_NAME_DESC"/>

	<field
			name="parent_id"
			type="number"
			label="COM_LANG4DEV_SUBPROJECT_PARENT_ID"
			description="COM_LANG4DEV_SUBPROJECT_PARENT_ID_DESC"
			default="0"
			class="readonly"
	/>

	<field
			name="twin_id"
			type="number"
			label="COM_LANG4DEV_SUBPROJECT_ORIGIN_TWIN_ID"
			description="COM_LANG4DEV_SUBPROJECT_ORIGIN_TWIN_ID_DESC"
			default="0"
			class="readonly"
	/>

	<!-- lang_path_type -->
	<!-- lang_ids -->

	<field
			name="lang_ids"
			type="textarea"
			label="COM_LANG4DEV_LANG_IDS"
			description="COM_LANG4DEV_LANG_IDS_DESC"
			size="40"
			maxlength="255"
			default=""
	/>

	<field
			name="params"
			type="textarea"
			label="COM_LANG4DEV_PARAMS"
			description="COM_LANG4DEV_PARAMS_DESC"
			size="40"
			maxlength="255"
			default=""
	/>






	<field
			name="extension"
			type="hidden"
	/>

	<field
			name="ordering"
			type="integer"
			filter="unset"
	/>

	<field
			name="created"
			type="calendar"
			label="JGLOBAL_CREATED_DATE"
			translateformat="true"
			showtime="true"
			size="22"
			filter="user_utc"
	/>

	<field
			name="created_by"
			type="user"
			label="JGLOBAL_FIELD_CREATED_BY_LABEL"
	/>

	<field
			name="created_by_alias"
			type="text"
			label="COM_LANG4DEV_CREATED_BY_ALIAS_LABEL"
			size="20"
	/>

	<field
			name="modified"
			type="calendar"
			label="JGLOBAL_FIELD_MODIFIED_LABEL"
			class="form-control"
			translateformat="true"
			showtime="true"
			size="22"
			readonly="true"
			filter="user_utc"
	/>

	<field
			name="modified_by"
			type="user"
			label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
			class="readonly"
			readonly="true"
			filter="unset"
	/>

	<field
			name="checked_out"
			type="hidden"
			filter="unset"
	/>

	<field
			name="checked_out_time"
			type="hidden"
			filter="unset"
	/>

	<field
			name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			size="1"
	/>

	<!-- componentPrefix, prjXmlPathFilename, installPathFilename, useLangSysIni -->


	<!-- ? $sourceLangFiles = [], $transIdLocations = [], $transIdsClassified; -->


</form>